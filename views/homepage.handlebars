<div class="container mt-4">
  <div class="row justify-content-center"> <!-- Flexbox utility to center the columns -->
    <div class="col-md-9">
      <div class="card-container">
        <h2>Current Cache</h2> 
        {{#each posts as |post|}}
          <div class="card mb-3"> 
            <div class="post-title mb-3">
              <h2>{{post.title}}</h2>
            </div>
            <p>{{post.body}}</p>
          </div>
        {{/each}}
      </div>
    </div>
  </div>
</div>
