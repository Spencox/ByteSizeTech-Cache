<header>
  <nav class="navbar bg-body-tertiary">
      <div class="container-fluid d-flex justify-1content-between align-items-center">
          <div class="navbar-brand">
              <a href="/">
                  <img src="images/Icon.png" alt="Logo" style="height: 75px;" class="mr-2">
              </a>
          </div>
          {{#if logged_in}}
              <a id="homepage" class="btn-link text-white font-weight-bold" href="/">Homepage</a>
              <a id="dashboard" class="btn-link text-white font-weight-bold" href="/dashboard">Dashboard</a>
              <a id="logout" class="btn btn-primary" href="/logout">Logout</a>
          {{else}}
                <a id="login" class="btn btn-primary" href="/login">Login</a>
          {{/if}}
      </div>
  </nav>
</header>
<main>
  <div class="container mt-4">
    <div class="row justify-content-center">
      <div class="col-md-9">
        <div class="card-container">
          <h2>Current Cache</h2> 
          {{#each posts as |post|}}
            <div class="card mb-3"> 
              <div class="post-title mb-3">
                <h2>{{post.title}}</h2>
              </div>
              <p>{{post.body}}</p>
            </div>
          {{/each}}
        </div>
      </div>
    </div>
  </div>
</main>
